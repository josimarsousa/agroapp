{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "views/**",
          "public/**",
          "package.json"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.js" }
  ]
}